<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
	<select id="QID_SELECT_USER" resultType="UserDto">
		SELECT
			*
		FROM
			ym_user
		<where>
			AND user_id = #{user_id}
		</where>
	</select>
</mapper>